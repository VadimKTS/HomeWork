@model List<IndexFormModel>
@{
    ViewData["Title"] = "Tape";
}
<h1>@ViewData["Title"]</h1>

<table class="table">
    <caption>Tape</caption>
    <tbody>
        @foreach (var p in Model)
        {
            <tr>
                <td>@p.LastDate @p.Name ел спаржу уже @p.Count раз</td>
            </tr>
        }
    </tbody>
</table>
<div>
    <a asp-action="Again" asp-controller="Home">Ещё спаржи</a>
</div>